#include <kipr/wombat.h>

int main()
{
    printf("Hello World\n");
    enable_servos();
    set_servo_position(0,325);
    msleep(3000);
    set_servo_position(1,1390);
    msleep(1000);
  
    motor(0,75);
    motor(1,75);
    motor(2,75);
    motor(3,75);
    msleep(3000);
    
    ao();
    msleep(100);
    
    set_servo_position(0,1460);
    msleep(2000);
    
    ao();
    msleep(100);
    
    set_servo_position(1,2047);
    msleep(1000);
    ao();
    msleep(1000);
    set_servo_position(0,833);
    msleep(1000);
    
    
    
    motor(0,-75);
    motor(1,-75);
    motor(2,-75);
    motor(3,-75);
    msleep(1300);
    
    motor(0,-75);
    motor(1,-75);
    motor(2,75);
    motor(3,75);
    msleep(1800);
    
    motor(0,75);
    motor(1,75);
    motor(2,75);
    motor(3,75);
    msleep(9900);
    
    motor(0,-75);
    motor(1,-75);
    motor(2,75);
    motor(3,75);
    msleep(1800);

    motor(0,75);
    motor(1,75);
    motor(2,75);
    motor(3,75);
    msleep(600);
    
    
    ao();
    msleep(100);
    
    set_servo_position(0,1700);
    msleep(1000);
    
    ao();
    msleep(100);
        set_servo_position(1,1600);
    msleep(1000);
    

       set_servo_position(0,2000);
    msleep(1000);
    ao();
    msleep(100);
    set_servo_position(1,2000);
    msleep(1000);

    disable_servos();
    
    return 0;
}
