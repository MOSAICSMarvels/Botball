#include <kipr/wombat.h>

int main()
{
    printf("Hello World\n");
    
      enable_servos();
    
    set_servo_position(0,1399);
    msleep(1000);
    
    set_servo_position(3,285);
    msleep(1000);
    
    ao();
    msleep(500);
    
    motor(0,99);
    motor(3,100);
    msleep(3150);
    
    ao();
    msleep(1000);
    
    set_servo_position(3,1179);
    msleep(1000);
    
    motor(3,-97);
    motor(0,-100);
    msleep(1800);
    
    ao();
    msleep(500);
    
    motor(3,100);
    motor(0,0);
    msleep(1500);
    
    ao();
    msleep(500);
    
    set_servo_position(0,655);
    msleep(1000);
    
    ao();
    msleep(500);
    
    motor(3,98);
    motor(0,100);
    msleep(1250);
    
    ao();
    msleep(500);
    
    set_servo_position(0,1500);
    msleep(1000);
    
    ao();
    msleep(500);
    
    set_servo_position(3,285);
    msleep(1000);
    
    motor(3,-98);
    motor(0,-100);
    msleep(1200);
    
       
    set_servo_position(0,1399);
    msleep(1000); 
    
    ao();
    msleep(500);
    
    set_servo_position(3,285);
    msleep(1000);
    
    motor(3,0);
    motor(0,100);
    msleep(1650);
    
    ao();
    msleep(500);
    
    set_servo_position(0,1775);
    msleep(1000);
    
    ao();
    msleep(500);
    
    motor(3,98);
    motor(0,100);
    msleep(1100);
    
    ao();
    msleep(500);
    
    set_servo_position(3,1361);
    msleep(1000);
    
    ao();
    msleep(500);
    
    motor(3,-98);
    motor(0,-100);
    msleep(2000);
    
    
    
    
    
    
    
    
    disable_servos();
    
    
    
