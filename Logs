int main()
{
    create_connect();
    wait_for_light(0);
    shut_down_in(119);
    
    create_drive_direct(300,300);
    msleep(1500);
    
    create_drive_direct(0,300);
    msleep(1550);
    
    
    create_drive_direct(300,0);
    msleep(1550);
    
    create_drive_direct(300,300);
    msleep(500);
    
    create_drive_direct(0,300);
    msleep(1400);
        
    return 0;
}
