
int main()
{
    enable_servos();
    set_servo_position(3,763);
    set_servo_position(0,315);
    
    motor(0,75);
    motor(3,75);
    msleep(2000);
    
    while (analog(0)<=2840);
    ao();
    msleep(1000);
    
    set_servo_position(3,1162);
    msleep(1000);
    set_servo_position(0,1300);
    ao();
    msleep(1000);
    disable_servos();
    
    motor(0,-75);
    motor(3,-75);
    msleep(2000);
    
    
    return 0;
}
