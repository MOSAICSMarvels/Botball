#include <kipr/wombat.h>
//Code to slow down your servo
int main()
{
    int count =0;
    int arm =0;
    int claw =1;
    int up= 800;
    int down = 1700;
    
    printf("Slow the servo!\n");
    enable_servos();
    set_servo_position(arm, 1400);
    count = 0;
    while(count <= 5)
    {
        up = up + 50;
     set_servo_position(arm,up); 
        msleep(100);
        
        count = count + 1;
    }
    
    return 0;
}
