#include <kipr/wombat.h>
//claw = 3
//open =300
//close = 1600
int main()
{
      
    int arm=0;
    int out=1297;
    int up = 590;
    int down = 1900;
    int claw = 3;
    int open = 350;
    int close = 1700;
    printf("Grab the cube!\n");
    enable_servos();
    set_servo_position(arm, out);
    set_servo_position(claw,open);
    msleep(500);
    
    motor(0,100);
    motor(3,100);
    msleep(3000);
    
    ao();
    msleep(500);
    
    set_servo_position(claw,close);
    msleep(500);
    
    disable_servos();
    return 0;
}
